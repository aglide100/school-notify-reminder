<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/PlanListFragment">

    <fragment
        android:id="@+id/PlanListFragment"
        android:name="com.example.myapplication.View.Fragment.PlanListFragment"
        android:label="PlanList Fragment"
        tools:layout="@layout/fragment_planlist">

    </fragment>

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.myapplication.View.Fragment.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.myapplication.View.Fragment.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <action
            android:id="@+id/action_SecondFragment_to_ThirdFragment"
            app:destination="@id/ThirdFragment" />
    </fragment>

    <fragment
        android:id="@+id/SettingFragment"
        android:name="com.example.myapplication.View.Fragment.SettingFragment"
        android:label="Setting Fragment"
        tools:layout="@layout/fragment_setting">

    </fragment>

    <fragment
        android:id="@+id/ThirdFragment"
        android:name="com.example.myapplication.View.Fragment.ThirdFragment"
        android:label="Third Fragment"
        tools:layout="@layout/fragment_third">

        <action
            android:id="@+id/action_ThirdFragment_to_FirstFragment"
            app:destination="@+id/FirstFragment"></action>
        <action
            android:id="@+id/action_ThirdFragment_to_SecondFragment"
            app:destination="@+id/SecondFragment"></action>
    </fragment>


</navigation>